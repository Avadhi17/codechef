#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, x, y;
    cin >> t;
    while (t--) {
        cin >> x >> y;
        int num = x % y;
        if (num == 0) {
            cout << x / y << endl;
        }
        else {
            cout << (x / y) + num << endl;
        }
    }

}
