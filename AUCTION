#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int a, b, c, t;
    cin >> t;
    while (t--) {
        cin >> a >> b >> c;
        if (a > b && a > c) {
            cout << "Alice" << endl;
        }
        else if (c > b && c > b) {
            cout << "Charlie" << endl;
        }

        else {
            cout << "Bob" << endl;
        }
    }
}
