#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {

        int x, y, z;
        cin >> x >> y >> z;

        int allstudnts = x * y;

        if( 2* z > allstudnts) {

            cout << "yes" << endl;
        }
        else {

            cout << "no" << endl;
        }

    }
    return 0;
}
