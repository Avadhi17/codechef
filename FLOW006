#include <iostream>

using namespace std;

int main() {
    int t, n;
    cin >> t;
    while (t--) {
        cin >> n;
        int remained, sum = 0;
        while(n > 0) {
            remained = n % 10;
            sum = sum + remained;
            n = n / 10;
        }
        cout << sum << endl;
      
    }
    return 0;
}
