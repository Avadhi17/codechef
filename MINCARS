#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    cin>>n;
	    int cars = n / 4; 
        if (n % 4 != 0) 
        { 
            cars += 1; 
            cout<< cars<<endl;
        }
        else{
        cout << cars << endl;
        }
    }
}
