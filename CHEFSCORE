#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, n, x, y;
    cin >> t;
    while (t--) {
        cin >> n >> x >> y;
        if (y > n * x || y % x != 0) {
            cout<<"no"<<endl;
        }
        else {
                cout << "yes" << endl;
        }
    }

}
