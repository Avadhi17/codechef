#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, x;
    cin >> t;
    while (t--) {
        cin >> x;
        int num = x % 3;
        if (num == 0) {
            cout << "NORMAL" << endl;
        }
        else if (num == 1) {
            cout << "HUGE" << endl;
        }
        else {
            cout << "SMALL" << endl;
        }

    }
    return 0;
}
