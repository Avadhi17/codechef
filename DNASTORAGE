#include <bits/stdc++.h>

#include<string>

using namespace std;

int main() {
    int t, n, i;
    string s;
    cin >> t;
    while (t--)
    {
        cin >> n;
        cin >> s;
        for (i = 0; i < n; i += 2) {
            if (s[i] == '0' && s[i + 1] == '0') {
                cout << 'A';
            }
            else if (s[i] == '0' && s[i + 1] == '1') {
                cout << 'T';
            }
            else if (s[i] == '1' && s[i + 1] == '0') {
                cout << 'C';
            }
            else {
                cout << 'G';
            }
          
        }   cout << endl;

    }
    return 0;
}
