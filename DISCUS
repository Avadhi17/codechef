#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, a, b, c;
    cin >> t;
    while (t--) {
        cin >> a >> b >> c;
        if (a >= b && a >= c) {
            cout << a << endl;
        }
        else if (c >= b && c >= a) {
            cout << c << endl;
        }
        else {
            cout << b << endl;
        }
    }
}
