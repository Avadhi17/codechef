#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t;
    cin >> t;
    while (t--) {
     int n,x,num =0 ;
        cin >> n >> x;
        for (int i = 0; i < n; i++) {
            int age;
            cin >> age;
            if (age >= x) {
                num++;
            }
            else{
                num=num;
            }
        }
        cout << num << endl;
    }
 return 0;
}
