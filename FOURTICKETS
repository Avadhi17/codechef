#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, x, cost;
    cin >> t;
    while (t--) {
        cin >> x;
        cost = x * 4;
        if (cost <= 1000) {
            cout << "yes" << endl;
        }
        else {
            cout << "no" << endl;
        }
    }

}
