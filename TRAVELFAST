#include <bits/stdc++.h>

using namespace std;

int main() {
    int t, x, y;
    cin >> t;
    while (t--) {
        cin >> x >> y;
        if (x < y) {
            cout << "bike" << endl;
        }
        else if (x > y) {
            cout << "car" << endl;
        }
        else {
            cout << "same" << endl;
        }
    }

}
