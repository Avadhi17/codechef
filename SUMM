#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, a, b, c;
    cin >> t;
    while (t--) {
        cin >> a >> b >> c;
        int sum = a + b;
        if (c == sum) {
            cout << "yes" << endl;
        }
        else {
            cout << "no" << endl;
        }
    }
}
