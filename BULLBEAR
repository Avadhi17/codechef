#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, x, y;
    cin >> t;
    while (t--) {
        cin >> x >> y;
        if (x > y) {
            cout << "LOSS" << endl;
        }
        else if (y == x) {
            cout << "NEUTRAL" << endl;
        }
        else {
            cout << "PROFIT" << endl;
        }
    }

}
