#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t, n, k, x;
    cin >> t;
    while (t--) {
        cin >> n >> x >> k;
        if (n * x <= k) {
            cout << "yes" << endl;
        }
        else {
            cout << "no" << endl;
        }
    }
}
